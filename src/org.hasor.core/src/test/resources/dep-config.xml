<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="http://myProject.org">
	<!-- 框架配置 -->
	<hasor>
		<!-- 包扫描覆盖范围，逗号间隔（支持重复配置共同生效） -->
		<loadPackages>org.hasor.test.dependency.*</loadPackages>
		<!-- 执行事件的线程池大小 -->
		<eventThreadPoolSize>1</eventThreadPoolSize>
		<forceModule>false</forceModule>
	</hasor>
	<environmentVar>
		<!-- 程序工作空间基础目录（绝对地址） -->
		<HASOR_WORK_HOME>./test-work</HASOR_WORK_HOME>
	</environmentVar>

	<userConfig>cc</userConfig>
</config>