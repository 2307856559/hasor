<beans staticCache="10" dynamicCache="50"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="beans-schema.xsd"
	xsi:schemaLocation="http://more.byshell.org/schema/anno anno-schema.xsd"
	xmlns:anno="http://more.byshell.org/schema/anno">

	<bean name="filter" type="org.moretest.filter.TestFilter">
		<meta key="isFilter" value="true"/>
	</bean>
	<bean name="publicFilter" type="org.moretest.filter.PublicFilter">
		<meta key="isFilter" value="true"/>
		<meta key="isPublicFilter" value="true"/>
	</bean>
	
	<bean name="action" type="org.moretest.filter.TestAction">
		<meta key="isAction" value="true" />
		<meta key="actionFilters" value="filter" />
	</bean>
	<bean name="action-filterLink" type="org.moretest.filter.TestAction">
		<meta key="isAction" value="true" />
		<meta key="actionFilters" value="filter,filter" />
	</bean>
</beans>