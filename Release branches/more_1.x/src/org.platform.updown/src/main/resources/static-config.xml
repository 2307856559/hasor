<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="http://project.byshell.org/more/schema/global">
	<!-- httpServlet文件上传服务配置 -->
	<upLoadService enable="false">
		<!-- name：服务名 -->
		<!-- maxSize：上传服务允许的最大请求数据长度。默认：0(无限制) -->
		<!-- minSize：上传服务允许的最小请求数据长度。默认：0(无限制) -->
		<!-- allowMulti：是否允许一个请求中多个上传实体请求 -->
		<!-- allowFiles：允许的文件类型 -->
		<upPoint name="" maxSize="0" minSize="" allowMulti="false" allowFiles="*.*" />
	</upLoadService>
</config>