<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://project.byshell.org/more/schema/beans"
	xmlns:config="http://project.byshell.org/more/schema/config">

	<templateBean name="1.testCollection">
		<!-- ==ArrayCollection== -->
		<property name="p_array">
			<array collectionType="int" initSize="12">
				<value value="123" />
				<value value="321" />
			</array>
		</property>
		<!-- ==ListCollection== -->
		<property name="p_list">
			<list>
				<value value="123" type="float" />
				<value value="321" type="double"/>
			</list>
		</property>
		<!-- ==SetCollection== -->
		<property name="p_set">
			<set>
				<value value="123" type="long"/>
				<value value="321" type="string"/>
			</set>
		</property>
		<!-- ==MapCollection== -->
		<property name="p_map">
			<map>
				<entity>
					<key></key>
					<var></var>
				</entity>
				<entity key="" value="123" />
			</map>
		</property>
	</templateBean>


	<!--===============================================================集合类型 -->
	<templateBean name="2.testCollection">
		<!-- ==ArrayCollection== -->
		<property name="p_array">
			<array initSize="12">
				<value value="123" />
				<value value="321" />
			</array>
		</property>
		<!-- ==ListCollection== -->
		<property name="p_list">
			<list>
				<value value="123"/>
				<value value="321"/>
				<uri uriLocation="http://www.google.com"/>
				<directory path="c:\windows\aa.bpm\"/>
			</list>
		</property>
		<!-- ==SetCollection== -->
		<property name="p_set">
			<set>
				<value value="123"/>
				<value value="321"/>
			</set>
		</property>
		<!-- ==MapCollection== -->
		<property name="p_map">
			<map>
				<!-- 第一个值为key第二个为value -->
				<entity>
					<!-- key --><directory path="c:\windows\aa.bpm\"/>
					<!-- var --><uri uriLocation="http://www.google.com"/>
				</entity>
				<entity key="key_1" value="var_1" />
				<entity key="key_2">
					<!-- var --><uri uriLocation="http://www.google.com"/>
				</entity>
				<entity value="var_2">
					<!-- key --><directory path="c:\windows\aa.bpm\"/>
				</entity>
			</map>
		</property>
	</templateBean>
</beans>