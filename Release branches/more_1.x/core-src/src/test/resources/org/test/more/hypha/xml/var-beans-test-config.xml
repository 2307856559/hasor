<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://project.byshell.org/more/schema/beans">
<!-- 
	<varBean
		id				="bean id"
		name			="bean name"
		package			="org.more"
		singleton		="true | false(Default)"
		lazy			="true(Default) | false"
		description		="description info"
		
		type="int"			value="12345"
	/>
-->
	<package package="org.more">
		<varBean name="v_bean_0"
		type="int"	value="12345"/>
	</package>

	<varBean id="v_bean_1"
		type="int" value="12345"/>

	<varBean name="v_bean_2" package="org.more"
		type="int" value="12345"/>

	<varBean id="v_bean_3" iocType="Ioc"
		type="int" value="12345"/>

	<varBean id="v_bean_4" singleton="true"
		type="int" value="12345"/>

	<varBean id="v_bean_5" lazy="false"
		type="int" value="12345"/>

	<varBean id="v_bean_6" description="描述信息..."
		type="int" value="12345"/>
		
	<!-- 数据类型测试 -->
	<varBean id="v_bean_7"  type="null"/>
	<varBean id="v_bean_8"  type="boolean" 	value="true"/>
	<varBean id="v_bean_9"  type="byte" 	value="123"/>
	<varBean id="v_bean_10" type="short" 	value="2345"/>
	<varBean id="v_bean_11" type="int" 		value="12345"/>
	<varBean id="v_bean_12" type="long" 	value="123457890"/>
	<varBean id="v_bean_13" type="float" 	value="123.45"/>
	<varBean id="v_bean_14" type="double" 	value="1.2345"/>
	<varBean id="v_bean_15" type="char" 	value="}"/>
	<varBean id="v_bean_16" type="string" 	value="测试字符串..."/>
</beans>